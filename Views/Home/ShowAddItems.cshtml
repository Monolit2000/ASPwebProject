
@model IEnumerable<WebApplication1.Models.CartItem>



<h2>Список пользователей</h2>
<p><a asp-action="AddItem">Добавить пользователя</a></p>
<table class="table">
     <tr><th>Имя</th><th>Возраст</th></tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ItemName</td>
            <td>@item.Price</td>
        </tr>
    }
</table>


